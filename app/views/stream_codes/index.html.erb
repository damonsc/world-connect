<p id="notice"><%= notice %></p>

<h1>Stream Codes</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Redirect</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stream_codes.each do |stream_code| %>
      <tr>
        <td><%= stream_code.code %></td>
        <td><%= stream_code.redirect %></td>
        <td><%= link_to 'Show', stream_code %></td>
        <td><%= link_to 'Edit', edit_stream_code_path(stream_code) %></td>
        <td><%= link_to 'Destroy', stream_code, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stream Code', new_stream_code_path %>
