<p id="notice"><%= notice %></p>
<%= render 'home/header' %>
<h1>Connect Codes</h1>

<table class="table">
  <thead>
    <tr>
      <th>Code</th>
      <th>Assigned</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @connect_codes.each do |connect_code| %>
      <tr>
        <td><%= connect_code.code %></td>
        <td><%= connect_code.assigned %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Connect Code', new_connect_code_path %>
